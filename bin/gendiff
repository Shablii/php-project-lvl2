#!/usr/bin/env php
<?php

require __DIR__ . '/../vendor/autoload.php';

$doc = <<<DOC
Generate diff

Usage:
  gendiff (-h|--help)
  gendiff (-v|--version)

Options:
  -h --help                     Show this screen
  -v --version                  Show version
DOC;

$result = Docopt::handle($doc, array('version' => '1'));
